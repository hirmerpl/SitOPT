	<!-- Complex types -->
	<complexType name="PersonalDataAttributeType">
	 <annotation>
		<documentation>a complex type that contains personal data of an human ressource</documentation>
	 </annotation>
	 <sequence>
		<element name="value">
			<complexType>
			 <sequence>
				<element name="personnelNumber" type="nsat:NexusIntegerType" minOccurs="0"/>
				<element name="roomNumber" type="nsat:NexusStringType" minOccurs="0"/>
				<element name="telephone" type="nsat:NexusStringType" minOccurs="0"/>
			 </sequence>
			</complexType>
		</element>
		<element ref="nsas:meta" minOccurs="0"/>
	 </sequence>
	</complexType>
	<complexType name="CustomerAgreementAttributeType">
	 <annotation>
		<documentation>a complex type that describes customer agreements</documentation>
	 </annotation>
	 <sequence>
		<element name="value">
			<complexType>
			 <sequence>
				<element name="contractPenalty" type="nsat:NexusFloatType" minOccurs="0"/>
				<element name="fictiveBadDeliveryCost" type="nsat:NexusFloatType" minOccurs="0"/>
				<element name="minDeliveryWindow" type="nsat:NexusTimeType" minOccurs="0"/>
				<element name="maxDeliveryWindow" type="nsat:NexusTimeType" minOccurs="0"/>
				<element name="minOrderPeriod" type="nsat:NexusIntegerType" minOccurs="0"/>
			 </sequence>
			</complexType>
		</element>
		<element ref="nsas:meta" minOccurs="0"/>
	 </sequence>
	</complexType>
	<complexType name="DocumentAttributeType">
	 <annotation>
		<documentation>?</documentation>
	 </annotation>
	 <sequence>
		<element name="value">
			<complexType>
			 <sequence>
				<element name="documentType" type="nsat:NexusStringType" minOccurs="0"/>
				<element name="documentFileLink" type="nsat:NexusUriType" minOccurs="0"/>
			 </sequence>
			</complexType>
		</element>
		<element ref="nsas:meta" minOccurs="0"/>
	 </sequence>
	</complexType>
	<complexType name="ErrorModelAttributeType">
	 <annotation>
		<documentation>a complex type that describes errors</documentation>
	 </annotation>
	 <sequence>
		<element name="value">
			<complexType>
