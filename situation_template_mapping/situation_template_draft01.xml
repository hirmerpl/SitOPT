<?xml version="1.0" encoding="UTF-8"?>
<SituationTemplate id="A0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="situation_template_draft01.xsd"
	name="SystemObservation">
	<Situation id="A1" name="SystemFailure">
		<logicNode id="A3" name="combine Sensors">
			<type>or</type>
		</logicNode>
		<node id="A4" name="% CPU load">
			<type>type</type>
			<measureName>measureName</measureName>
			<opType>greaterThan</opType>
			<condValues>
				<val>70</val>
			</condValues>
			<parent parentID="A3"/>
		</node>
		<node id="A8" name="MB RAM free">
			<type>type</type>
			<measureName>measureName</measureName>
			<opType>lowerThan</opType>
			<condValues>
				<val>10</val>
			</condValues>
			<parent parentID="A3"/>
		</node>
		<node id="A9">
			<type>type</type>
			<measureName>measureName</measureName>
			<opType>notStatusCode</opType>
			<condValues>
				<val>200</val>
			</condValues>
			<parent parentID="A3"></parent>
		</node>
		<sensorNode id="A5" name="memorySensor">
			<parent parentID="A8"></parent>
		</sensorNode>
		<sensorNode id="A6" name="cpuSensor">
			<parent parentID="A4"></parent>
		</sensorNode>
		<sensorNode id="A7" name ="watchdogSensor">
			<parent parentID="A9"/>
		</sensorNode>
	</Situation>
</SituationTemplate>
